---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  
  <!-- Home Pages -->
  <url>
    <loc>{{ site.url }}/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>1.0</priority>
  </url>
  <url>
    <loc>{{ site.url }}/en/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>1.0</priority>
  </url>

  <!-- Pages -->
  {% for page in site.pages %}
    {% if page.path contains '.html' %}
      {% unless page.path contains '_layouts' or page.path contains '_includes' or page.path contains 'projects/' %}
        {% if page.path contains 'fr/' or page.path contains 'en/' %}
  <url>
    <loc>{{ site.url }}{{ page.url }}</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <changefreq>{% if page.url contains 'portfolio' %}weekly{% else %}monthly{% endif %}</changefreq>
    <priority>{% if page.url contains 'portfolio' %}0.9{% else %}0.8{% endif %}</priority>
  </url>
        {% endif %}
      {% endunless %}
    {% endif %}
  {% endfor %}

  <!-- Projects -->
  {% for page in site.pages %}
    {% if page.path contains 'projects/' and page.path contains '.html' %}
      {% if page.path contains 'fr/projects/' or page.path contains 'en/projects/' %}
  <url>
    <loc>{{ site.url }}{{ page.url }}</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.7</priority>
  </url>
      {% endif %}
    {% endif %}
  {% endfor %}

</urlset>
